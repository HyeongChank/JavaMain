package CodeTest;

public class VowelRemove {
	static String answer = "";
	static String my_string = "bus";
	static int top =0;
	static String [] ms = my_string.split("");
	static String [] st = new String[ms.length] ;
	
	static void pushq(String str) {
		st[top++] = str;

	}
	static void remove(String [] str, int i){
		st[i] = " ";
	}
	public static void main(String[] args) {
		String [] vowel = {"a","e","i","o","u"};
		int h = 0;
		for(int i =0; i<ms.length; i++) {
			pushq(ms[i]);
			
			for(int j =0; j<vowel.length; j++) {
				if(ms[i].equals(vowel[j])) {
					remove(st,i);
				}
			}
			
		}
	
		for(int i =0; i<st.length;i++) {
			System.out.println(st[i]);
		}
	}

}
