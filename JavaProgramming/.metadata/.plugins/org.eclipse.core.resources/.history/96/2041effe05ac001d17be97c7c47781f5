package DataStructure_Queen;

import java.util.Scanner;

public class Recur {

	
	static void recur(int n){
		while(n>0) {
			recur(n-1);
			System.out.println(n);
			n = n-2;
		}
	}
	public static void main(String[] args) {
		Scanner kb = new Scanner(System.in);
		int n = kb.nextInt();
		recur(n);

	}

}
