package CodeTest;

public class QuickSort {

	static int [] swap(int[] arr, int ln, int rn) {
		int temp = arr[ln];
		arr[ln] = arr[rn];
		arr[rn] = temp;
		return arr;
	}
	
	static void print(int []arr) {
		for(int i:arr) {
			System.out.print(i + "  ");
		}
	}
	public static void main(String[] args) {
		int basic [] = {4,3,6,7,5,2,1,8};
		print(swap(basic, 0, 1));

	}

}
