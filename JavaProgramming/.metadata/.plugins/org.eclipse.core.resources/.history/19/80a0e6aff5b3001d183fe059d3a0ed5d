package CodeTest;

public class ArrayEqual {

	public static void main(String[] args) {
		String [] s1 = {"a","b","c"};
		String [] s2 = {"adcd","b","c"};
		int answer = 0;
		int num =0;
		for(int i=0 ; i<s1.length; i++) {
			for(int j=0; j<s2.length; j++) {
				if(s1[i] == s2[j]) {
					num++;
				}
			}
		}
		System.out.println(num);
		
		

	}

}
