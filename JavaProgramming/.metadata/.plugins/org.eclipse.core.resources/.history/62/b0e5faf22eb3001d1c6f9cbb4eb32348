package CodeTest;

public class NumAdd {

	public static boolean check(char[] ch) {
		for(int i=0; i<ch.length;i++) {
			print(ch[i]);
			print((int)ch[i]);
//			if((int)ch[i]<10) {
//				print(ch[i]);
//			}
			
		}
		System.out.println();
		return true;
	}
	
	private static void print(int c) {
		System.out.print(c);
		
		
	}

	public static void main(String[] args) {
		String my_string = "aAb1B2cC34oOp";
		char [] ch = my_string.toCharArray();
		
		check(ch);
		

	}

}
