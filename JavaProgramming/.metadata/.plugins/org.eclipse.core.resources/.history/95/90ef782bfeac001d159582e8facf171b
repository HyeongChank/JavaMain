package CodeTest;

public class 문자숫자변환 {

	public static void main(String[] args) {
		int n = 12345;
		int answer =0;
		char e = 'a';
		String f = "fd";
		
		//숫자 문자열 변환
		String b = String.valueOf(n);
		String d = Integer.toString(n);
		//문자열 숫자 변환
		int a = Integer.valueOf(b);
		
		//char 문자열 변환
		f = Character.toString(e);
		
		//문자열 char [] 변환
		char [] charry = f.toCharArray();
		 // int -> char 변환
		answer = (int)e;
		
		System.out.println("d"+d);
		String [] str = b.split("");
		for(String i : str) {
			int c = Integer.valueOf(i);
			answer += c;
		}
		System.out.println(answer);

	}

}
