package CodeTest;

public class InsertSort {

	static int [] swap(int a[], int m, int n) {
		int arr[] = a;
		int min = m;
		int num = n;
		int temp=0 ;
		temp = a[min];
		a[min] = a[num];
		a[num] = temp;
		
		return arr;
	}
	
	
	static void print(int [] arr) {
		int [] result = arr;
		for(int i=0; i<result.length; i++) {
			System.out.print(result[i] + " ");
		}
	}
	
	public static void main(String[] args) {
		int [] basic = {1,6,3,5,4,2};
		int min = 1;
		int num = 2;
		
		print(swap(basic, min, num));
	}

}
