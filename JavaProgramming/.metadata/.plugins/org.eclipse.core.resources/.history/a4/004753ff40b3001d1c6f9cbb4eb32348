package CodeTest;

public class VowelRemove {
	static int top =0;
	static String [] st = new String[10] ;
	static String [] st2 = new String[10] ;
	
	static void pushq(String str) {
		st[top++] = str;

	}
	static String popq(String [] str){
		return str[--top];
	}
	public static void main(String[] args) {
		String answer = "";
		String my_string = "bus";
		String [] vowel = {"a","e","i","o","u"};
		
		String [] ms = my_string.split("");
		int h = 0;
		for(int i =0; i<ms.length; i++) {
			st2[i] = ms[i];
			for(int k=0; k<10;k++) {
				for(int j =0; j<vowel.length; j++) {
					if(ms[i].equals(vowel[j])) {
						pushq(ms[i]);
						popq(st2);
					}
				}
			}
		}
	
		for(String i : st2) {
			System.out.println(i);
		}
	}

}
